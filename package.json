{
  "name": "bankascanner",
  "version": "0.0.12",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agmitron/bankascanner.git"
  },
  "description": "Parse your PDF bank statements into structured format.",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig.build.json && tsc-alias",
    "start": "tsx cli",
    "test": "vitest",
    "test:watch": "vitest watch",
    "postinstall": "patch-package",
    "lint": "biome lint ./",
    "lint:fix": "biome lint --write ./",
    "format": "biome format --write ./",
    "prepare": "husky",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.5.4",
    "@types/yargs": "^17.0.33",
    "husky": "^9.1.7",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.14",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "vite-tsconfig-paths": "^5.1.1",
    "vitest": "^2.1.4"
  },
  "dependencies": {
    "@types/papaparse": "^5.3.15",
    "buffer": "^6.0.3",
    "fs": "^0.0.1-security",
    "papaparse": "^5.5.2",
    "patch-package": "^8.0.0",
    "pdfjs-dist": "^4.6.82",
    "yargs": "^17.7.2"
  }
}
