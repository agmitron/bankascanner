{
  "name": "bankascanner",
  "version": "0.0.23",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agmitron/bankascanner.git"
  },
  "description": "Parse your bank statements from PDF (or other input) into structured format.",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./importer": {
      "types": "./dist/importer/index.d.ts",
      "require": "./dist/importer/index.js",
      "default": "./dist/importer/index.js"
    },
    "./importer/*": {
      "types": "./dist/importer/*/index.d.ts",
      "require": "./dist/importer/*/index.js",
      "default": "./dist/importer/*/index.js"
    },
    "./exporter": {
      "types": "./dist/exporter/index.d.ts",
      "require": "./dist/exporter/index.js",
      "default": "./dist/exporter/index.js"
    },
    "./exporter/*": {
      "types": "./dist/exporter/*/index.d.ts",
      "require": "./dist/exporter/*/index.js",
      "default": "./dist/exporter/*/index.js"
    },
    "./lib": {
      "types": "./dist/lib/index.d.ts",
      "require": "./dist/lib/index.js",
      "default": "./dist/lib/index.js"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "require": "./dist/lib/*.js",
      "default": "./dist/lib/*.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "importer": [
        "dist/importer/index.d.ts"
      ],
      "importer/*": [
        "dist/importer/*/index.d.ts"
      ],
      "exporter": [
        "dist/exporter/index.d.ts"
      ],
      "exporter/*": [
        "dist/exporter/*/index.d.ts"
      ],
      "lib": [
        "dist/lib/index.d.ts"
      ],
      "lib/*": [
        "dist/lib/*.d.ts"
      ],
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -b tsconfig.build.json",
    "postbuild": "tsc-alias -p tsconfig.build.src.json --outDir dist && tsc-alias -p tsconfig.build.lib.json --outDir dist/lib",
    "start": "tsx cli",
    "test": "vitest",
    "test:watch": "vitest watch",
    "postinstall": "patch-package",
    "lint": "biome lint ./",
    "lint:fix": "biome lint --write ./",
    "format": "biome format --write ./",
    "prepare": "husky",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.5.4",
    "@types/yargs": "^17.0.33",
    "husky": "^9.1.7",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "vite-tsconfig-paths": "^5.1.1",
    "vitest": "^2.1.4"
  },
  "dependencies": {
    "@types/papaparse": "^5.3.15",
    "buffer": "^6.0.3",
    "papaparse": "^5.5.2",
    "patch-package": "^8.0.0",
    "pdfjs-dist": "^4.6.82",
    "yargs": "^17.7.2"
  }
}
